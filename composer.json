{
    "name": "compasspointmedia/julietmenu",
    "description": "A Simple Menu Generator from Database.  Forked from secrethash/dropmenu",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Fullman",
            "email": "sam-git@samuelfullman.com"
        }
    ],
    "require": {
        "illuminate/console": "^6.0",
        "illuminate/support": "^6.0",
        "illuminate/cache": "^6.0"
    },
    "autoload": {
        "classmap": [
            "src/commands",
            "src/Model"
        ],
        "psr-4": {
            "Compasspointmedia\\Julietmenu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Compasspointmedia\\Julietmenu\\JulietmenuServiceProvider"
            ],
            "aliases": {
                "Julietmenu": "Compasspointmedia\\Julietmenu\\JulietmenuFacade"
            }
        }
    }
}
