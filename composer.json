{
    "name": "compasspointmedia/dropmenu",
    "description": "A Simple Menu Generator from Database.  Forked from secrethash/dropmenu",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Fullman",
            "email": "sam-git@samuelfullman.com"
        }
    ],
    "require": {
        "illuminate/console": "^5.0",
        "illuminate/support": "^5.0",
        "illuminate/cache": "^5.0"
    },
    "autoload": {
        "classmap": [
            "src/commands",
            "src/Model"
        ],
        "psr-4": {
            "Secrethash\\Dropmenu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Secrethash\\Dropmenu\\DropmenuServiceProvider"
            ],
            "aliases": {
                "Dropmenu": "Secrethash\\Dropmenu\\DropmenuFacade"
            }
        }
    }
}
